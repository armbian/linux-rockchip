&reserved_memory {
	ramoops: ramoops@110000 {
		compatible = "ramoops";
		reg = <0x0 0x110000 0x0 0xf0000>;
		record-size = <0x20000>;
		console-size = <0x80000>;
		ftrace-size = <0x00000>;
		pmsg-size = <0x50000>;
	};

	rknpu_reserved: rknpu {
		compatible = "shared-dma-pool";
		inactive;
		reusable;
		size = <0x0 0x20000000>;
		alignment = <0x0 0x1000>;
	};
};

